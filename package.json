{
  "name": "react-playground",
  "version": "0.0.1",
  "description": "A place to play around with React and have a sense of source control",
  "main": "playground",
  "scripts": {
    "playground": "webpack-dev-server --config playground/webpack.config.js --content-base playground",
    "test": "jest"
  },
  "authors": [
    "Fernando Marcheselli"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.0.12",
    "babel-core": "^5.0.12",
    "babel-jest": "^6.0.1",
    "babel-loader": "^5.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bundle-loader": "^0.5.2",
    "css-loader": "^0.16.0",
    "events": "1.0.2",
    "expect": "^1.1.0",
    "file-loader": "^0.8.4",
    "font-awesome-webpack": "^0.0.3",
    "jest-cli": "^0.8.2",
    "jsxhint": "^0.12.1",
    "less-loader": "2.2.x",
    "marked": "0.3.3",
    "pygmentize-bundled": "^2.3.0",
    "react-addons-test-utils": "^0.14.3",
    "rf-changelog": "^0.4.0",
    "rx": "2.3.18",
    "style-loader": "0.12.x",
    "url-loader": "^0.5.6",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "classnames": "^2.1.5",
    "object-assign": "^2.0.0",
    "qs": "2.4.1",
    "react": "0.14.x",
    "react-dom": "^0.14.3"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component",
    "playground"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
